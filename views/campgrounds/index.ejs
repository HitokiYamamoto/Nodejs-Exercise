<% layout('/layouts/boilerplate') -%>

    <body>
        <h1>キャンプ場一覧</h1>
        <div>
            <a href="campgrounds/new">新規登録</a>
        </div>
        <ul>
            <% for(let campground of campgrounds) { %>
                <a href="/campgrounds/<%= campground._id %>">
                    <li>
                        <%= campground.title %>
                    </li>
                </a>
                <% } %>
        </ul>
    </body>

    </html>